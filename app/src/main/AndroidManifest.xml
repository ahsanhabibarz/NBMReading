<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.habii.nbmreadingapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_rnd"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_rnd"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".AllProfileActivity"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity
            android:name=".StudentsClassRoomActivity"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity
            android:name=".TeachersActivity"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity
            android:name=".StudentsBookActivity"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity
            android:name=".AllReadingActivity"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity
            android:name=".TeachersClassActivityConFrag"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity
            android:name=".StudentsActivity"
            android:theme="@style/AppTheme.NoActionbar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".ReadByActivity"
            android:theme="@style/AppTheme.NoActionbar"></activity>
    </application>

</manifest>